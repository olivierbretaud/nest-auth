  "info": {
    "title": "nest-auth",
    "description": "Documentation de mon API nest-auth",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "loginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "Adresse email utilisateur",
            "example": "johndoe@gmail.com"
          },
          "password": {
            "type": "string",
            "example": "P@ssword123456"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "loginResponseDto": {
        "type": "object",
        "properties": {
          "access_token": {
            "type": "string",
            "example": "token"
          }
        },
        "required": [
          "access_token"
        ]
      },
      "ErrorUnauthorizedDto": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "example": 401
          },
          "message": {
            "type": "string",
            "example": "Unauthorized"
          },
          "error": {
            "type": "string",
            "example": "Unauthorized"
          }
        },
        "required": [
          "statusCode",
          "message",
          "error"
        ]
      },
      "ErrorTooManyRequestsDto": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "example": 429
          },
          "message": {
            "type": "string",
            "example": "ThrottlerException: Too Many Requests"
          },
          "error": {
            "type": "string",
            "example": "Too Many Requests"
          }
        },
        "required": [
          "statusCode",
          "message",
          "error"
        ]
      },
      "RequestResetPasswordDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "Adresse email utilisateur",
            "example": "johndoe@gmail.com"
          }
        },
        "required": [
          "email"
        ]
      },
      "RequestResetPassworResponseDto": {
        "type": "object",
        "properties": {
          "resetToken": {
            "type": "string",
            "example": "token"
          },
          "message": {
            "type": "string",
            "example": "Un lien de réinitialisation a été envoyé."
          }
        },
        "required": [
          "resetToken",
          "message"
        ]
      },
      "ResetPasswordDto": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string",
            "example": "token"
          },
          "newPassword": {
            "type": "string",
            "example": "P@ssword123456"
          }
        },
        "required": [
          "token",
          "newPassword"
        ]
      },
      "ResetPassworResponseDto": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "example": "Mot de passe réinitialisé avec succès"
          }
        },
        "required": [
          "message"
        ]
      },
      "ErrorBadRequestDto": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "example": 400
          },
          "message": {
            "type": "string",
            "example": "Bad Request"
          },
          "error": {
            "type": "string",
            "example": "Bad Request"
          }
        },
        "required": [
          "statusCode",
          "message",
          "error"
        ]
      },
      "UserResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "example": 1
          },
          "email": {
            "type": "string",
            "example": "john.doe@example.com"
          },
          "firstName": {
            "type": "string",
            "example": "John"
          },
          "lastName": {
            "type": "string",
            "example": "Doe"
          },
          "role": {
            "type": "string",
            "enum": [
              "Admin",
              "Member"
            ],
            "example": "Member"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string",
            "example": "2024-01-01T10:00:00.000Z"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string",
            "example": "2024-01-01T10:00:00.000Z"
          }
        },
        "required": [
          "id",
          "email",
          "firstName",
          "lastName",
          "role",
          "createdAt",
          "updatedAt"
        ]
      },
      "ErrorForbiddenDto": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "example": 403
          },
          "message": {
            "type": "string",
            "example": "Forbidden"
          },
          "error": {
            "type": "string",
            "example": "Forbidden"
          }
        },
        "required": [
          "statusCode",
          "message",
          "error"
        ]
      },
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "john@mail.com"
          },
          "firstName": {
            "type": "string",
            "example": "John"
          },
          "lastName": {
            "type": "string",
            "example": "Doe"
          },
          "role": {
            "type": "string",
            "example": "Member"
          },
          "isActive": {
            "type": "boolean",
            "example": true
          }
        },
        "required": [
          "email",
          "firstName",
          "lastName",
          "role",
          "isActive"
        ]
      },
      "UpdateUserDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "john@mail.com"
          },
          "firstName": {
            "type": "string",
            "example": "John"
          },
          "lastName": {
            "type": "string",
            "example": "Doe"
          },
          "role": {
            "type": "string",
            "example": "Member"
          },
          "isActive": {
            "type": "boolean",
            "example": true
          }
        }
      },
      "ErrorNotFoundDto": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "example": 404
          },
          "message": {
            "type": "string",
            "example": "Resource not found"
          },
          "error": {
            "type": "string",
            "example": "Not Found"
          }
        },
        "required": [
          "statusCode",
          "message",
          "error"
        ]
      },
      "ErrorConflictDto": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "example": 409
          },
          "message": {
            "type": "string",
            "example": "Email already exists"
          },
          "error": {
            "type": "string",
            "example": "Conflict"
          }
        },
        "required": [
          "statusCode",
          "message",
          "error"
        ]
      }
    }
  }
