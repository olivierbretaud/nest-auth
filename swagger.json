  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "loginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "Adresse email utilisateur",
            "example": "johndoe@gmail.com"
          },
          "password": {
            "type": "string",
            "example": "P@ssword123456"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "loginResponseDto": {
        "type": "object",
        "properties": {
          "access_token": {
            "type": "string",
            "example": "token"
          }
        },
        "required": [
          "access_token"
        ]
      },
      "ErrorUnauthorizedDto": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "example": 401
          },
          "message": {
            "type": "string",
            "example": "Invalid credentials"
          },
          "error": {
            "type": "string",
            "example": "Unauthorized"
          }
        },
        "required": [
          "statusCode",
          "message",
          "error"
        ]
      },
      "RequestResetPasswordDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "Adresse email utilisateur",
            "example": "johndoe@gmail.com"
          }
        },
        "required": [
          "email"
        ]
      },
      "ResetPasswordDto": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string",
            "example": "token"
          },
          "newPassword": {
            "type": "string",
            "example": "P@ssword123456"
          }
        },
        "required": [
          "token",
          "newPassword"
        ]
      },
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "john@mail.com"
          },
          "firstName": {
            "type": "string",
            "example": "John"
          },
          "lastName": {
            "type": "string",
            "example": "Doe"
          },
          "password": {
            "type": "string",
            "example": "MotDePasse123"
          },
          "role": {
            "type": "string",
            "example": "Member"
          }
        },
        "required": [
          "email",
          "firstName",
          "lastName",
          "password",
          "role"
        ]
      }
    }
  }
